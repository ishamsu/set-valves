{
	"name": "trawild-website",
	"packageManager": "yarn@3.5.0",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"check-types": "tsc --noemit",
		"eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"next-lint": "next lint . --ext .js,.jsx,.ts,.tsx",
		"lint": "yarn eslint && yarn check-types",
		"format": "prettier --check \"src/**/*.ts\"",
		"postinstall": "husky install",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"bump-version": "npm version --no-git-tag-version",
		"get-next-version": "ts-node -P tsconfig.scripts.json ./.standard-version/commands/ComputeNextVersion.ts",
		"release-branch": "git flow release start $(ts-node -P tsconfig.scripts.json ./.standard-version/commands/ComputeNextVersion.ts)",
		"release": "standard-version"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"eslint --fix"
		]
	},
	"standard-version": {
		"skip": {
			"tag": true
		},
		"scripts": {
			"postcommit": "ts-node -P tsconfig.scripts.json ./.standard-version/commands/FixGitFlowReleaseBranchName.ts"
		}
	},
	"dependencies": {
		"@gsap/react": "^2.1.2",
		"@radix-ui/react-progress": "^1.1.6",
		"@radix-ui/react-separator": "^1.1.6",
		"@radix-ui/react-slot": "^1.2.2",
		"@radix-ui/react-tabs": "^1.1.11",
		"@tabler/icons-react": "^3.34.1",
		"@tailwindcss/typography": "^0.5.9",
		"@types/node": "18.16.1",
		"@types/react": "^19.0.10",
		"@types/react-dom": "18.2.1",
		"@vercel/analytics": "^1.0.0",
		"autoprefixer": "10.4.14",
		"axios": "^1.8.2",
		"balloons-js": "^0.0.3",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"dotted-map": "^2.2.3",
		"eslint": "8.39.0",
		"eslint-config-next": "14.1.0",
		"framer-motion": "^12.23.12",
		"geist": "^1.3.1",
		"gsap": "^3.13.0",
		"lenis": "^1.3.11",
		"lucide-react": "^0.508.0",
		"motion": "^12.23.12",
		"next": "14.1.0",
		"next-themes": "^0.2.1",
		"postcss": "8.4.23",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "^5.5.0",
		"react-parallax": "^3.5.2",
		"react-player": "^3.3.2",
		"reading-time": "^1.5.0",
		"tailwind-merge": "^3.2.0",
		"tailwindcss": "3.3.2",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "5.1.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-conventional": "^17.7.0",
		"@types/conventional-recommended-bump": "^6.1.0",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"autoprefixer": "^10.4.20",
		"conventional-recommended-bump": "^7.0.1",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"postcss": "^8.4.47",
		"prettier": "^3.0.2",
		"semver": "^7.5.4",
		"simple-git": "^3.19.1",
		"standard-version": "^9.5.0",
		"tailwindcss": "^3.4.14",
		"tailwindcss-textshadow": "^2.1.3",
		"ts-node": "^10.9.1"
	},
	"volta": {
		"node": "18.18.0",
		"yarn": "3.6.3"
	}
}
